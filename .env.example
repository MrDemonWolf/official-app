# WordPress REST API base URL
EXPO_PUBLIC_WORDPRESS_API_URL=https://staging.mrdemonwolf.com/wp-json/wp/v2

# WordPress user ID for the About screen profile (defaults to 1)
EXPO_PUBLIC_WORDPRESS_USER_ID=1

# App variant: "development" or "production"
EXPO_PUBLIC_APP_VARIANT=development

# Gravity Forms REST API base URL (for the contact form)
EXPO_PUBLIC_GF_API_URL=https://staging.mrdemonwolf.com/wp-json/gf/v2

# Gravity Forms contact form ID
EXPO_PUBLIC_GF_CONTACT_FORM_ID=1

# TailSignal push notification REST API base URL
# Used to register/unregister devices for push notifications via the TailSignal WordPress plugin
EXPO_PUBLIC_TAILSIGNAL_API_URL=https://staging.mrdemonwolf.com/wp-json/tailsignal/v1
